@page "/"
@*@inject IDemo _demo
    @inject IUtcDemo _demo2*@

@inject IEnumerable<IDemo> _demos
@inject ProcessDemo _processDemo

@foreach (var demo in _demos)
{
    @if (demo is ILocalDemo)
    {
        <h2>Local Demo: @demo.StartupTime.ToString("hh:mm:ss ffffff")</h2>

    }
    @if (demo is IUtcDemo)
    {
        <h2>Utc Demo: @demo.StartupTime.ToString("hh:mm:ss ffffff")</h2>

    }
}

<h1>Hello, world!</h1>
@*<h2>Demo Time: @_demo.StartupTime.ToString("hh:mm:ss ffffff")</h2>
    <h2>Demo2 Time: @_demo2.StartupTime.ToString("hh:mm:ss ffffff")</h2>*@
<h3>Days in Month: @_processDemo.GetDaysInMonth()</h3>
Welcome to your new app.

